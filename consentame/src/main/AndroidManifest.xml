<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.consenta.android.consentame">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application>
        <activity android:name=".activity.ConsentDetailsActivity" android:exported="false">
            <intent-filter android:order="1">
                <!--<action android:name="android.intent.action.VIEW" />-->

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="consenta.me"
                    android:pathPattern="/.*"
                    android:scheme="https" />
            </intent-filter>
        </activity>

        <uses-library android:name=".ConsentaMe"/>
        <uses-library android:name=".ConsentaMeCheckButton"/>

    </application>

</manifest>